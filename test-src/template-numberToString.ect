var expect = require("chai").expect;
var numberToString = require("../lib/numberToString");

describe("numberToString", function () {

<% for c in @testCases : %>
  it(<%- c.name %>, function () {
    var actual = numberToString(<%- c.num %>, <%- c.flags %>, <%- c.width %>, <%- c.prec %>, <%- c.type %>);
    expect(actual).equal(<%- c.expected %>);
  });

<% end %>
});


